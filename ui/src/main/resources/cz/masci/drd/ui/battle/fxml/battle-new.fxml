<BorderPane fx:id="mainPane" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
   <bottom>
      <AnchorPane prefHeight="37.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btnPrev" onAction="#onPrev" layoutX="14.0" layoutY="100.0" mnemonicParsing="false" text="Button" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0" />
            <Button fx:id="btnNext" onAction="#onNext" layoutX="134.0" layoutY="101.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="52.0" text="Button" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" />
         </children>
      </AnchorPane>
   </bottom>
    <center>
        <AnchorPane fx:id="centerPane" BorderPane.alignment="CENTER"/>
    </center>
   <top>
      <Pane BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="lblTitle" text="Label">
               <padding>
                  <Insets bottom="5.0" left="10.0" right="5.0" top="5.0" />
               </padding>
            </Label>
         </children>
      </Pane>
   </top>
</BorderPane>
